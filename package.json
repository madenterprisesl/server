{
  "name": "server",
  "version": "1.0.0",
  "description": "El E-Commerce de los E-Commerces",
  "main": "servidor.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madenterprisesl/server.git"
  },
  "homepage": "https://github.com/madenterprisesl/server#readme",
  "bugs": {
    "url": "https://github.com/madenterprisesl/server/issues"
  },
  "scripts": {
    "start": "node servidor.js",
    "server": "nodemon servidor.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "npm install --prefix client        && npm run build --prefix client"
  },
  "keywords": [],
  "author": "Alejandro Fernández Pérez",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "2.0.21"
  },
  "dependencies": {
    "bcrypt": "5.1.0",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-validator": "6.15.0",
    "helmet": "6.0.1",
    "jose": "4.13.1",
    "mysql2": "3.2.0"
  },
  "imports": {
    "#Config/*": [
      "./src/config/*"
    ],
    "#Ctes/*": [
      "./src/ctes/*"
    ],
    "#Db/*": [
      "./src/db/*"
    ],
    "#PtoControl/*": [
      "./src/ptoControl/*"
    ],
    "#RandomIDs/*": [
      "./src/RandomIDs/*"
    ],
    "#Routes/*": [
      "./src/routes/*"
    ],
    "#Servidor": [
      "./servidor.js"
    ]
  }
}
