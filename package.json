{
  "name": "server",
  "version": "1.0.0",
  "description": "El E-Commerce de los E-Commerces",
  "main": "index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madenterprisesl/server.git"
  },
  "homepage": "https://madenterprise.com",
  "bugs": {
    "url": "https://github.com/madenterprisesl/server/issues"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -b main -d build",
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider build"
  },
  "keywords": [],
  "author": "Alejandro Fernández Pérez",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "2.0.21"
  },
  "dependencies": {
    "bcrypt": "5.1.0",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-validator": "6.15.0",
    "gh-pages": "5.0.0",
    "helmet": "6.0.1",
    "jose": "4.13.1",
    "mysql2": "3.2.0",
    "react-scripts": "5.0.1"
  },
  "imports": {
    "#Config/*": [
      "./src/config/*"
    ],
    "#Ctes/*": [
      "./src/ctes/*"
    ],
    "#Db/*": [
      "./src/db/*"
    ],
    "#PtoControl/*": [
      "./src/ptoControl/*"
    ],
    "#RandomIDs/*": [
      "./src/RandomIDs/*"
    ],
    "#Routes/*": [
      "./src/routes/*"
    ],
    "#Servidor": [
      "./src/index.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
